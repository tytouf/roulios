{
    "llvm-target": "thumbv7m-none-eabi",
    "target-endian": "little",
    "target-pointer-width": "32",
    "target-os": "none",
    "target-env": "bare",
    "target-vendor": "bare",
    "arch": "arm",

    "data-layout": "e-m:e-p:32:32-i1:8:32-i8:8:32-i16:16:32-i64:64-v128:64:128-a:0:32-n32-S64",
    "pre-link-args": "",
    "post-link-args": "",
    "pre-link-objects": "",
    "post-link-objects": "",
    "cpu": "cortex-m3",
    "features": "+thumb2",
    "dynamic-linking": false,
    "executables": true,
    "relocation-model": "static",
    "code-model": "default",
    "disable-redzone": true,
    "eliminate-frame-pointer": true,
    "function-sections": true,
    "linker-is-gnu": true,
    "no-compiler-rt": true,
    "no-default-libraries": true,
    "allow-asm": true,
    "custom-unwind-resume": true,
    "lib-allocation-crate": "myalloc",
    "os": "none"
}
